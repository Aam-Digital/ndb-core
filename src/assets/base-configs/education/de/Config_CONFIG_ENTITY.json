          "label": "Team involved",
          "dataType": "entity",
          "isArray": true,
          "additional": "User",
          "defaultValue": {
            "mode": "dynamic",
            "config": {
              "value": "$current_user"
            }
          },
          "anonymize": "retain"
        },
        "category": {
          "label": "Kategorie",
          "dataType": "configurable-enum",
          "additional": "interaction-type",
          "anonymize": "retain"
        },
        "attachment": {
          "label": "Anhang",
