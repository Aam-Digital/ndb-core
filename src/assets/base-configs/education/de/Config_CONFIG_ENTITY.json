    "view:note": {
      "component": "NotesManager",
      "config": {
        "title": "Notizen",
        "includeEventNotes": false,
        "showEventNotesToggle": true,
        "columnGroups": {
          "default": "Standard",
          "mobile": "Mobil",
          "groups": [
            {
              "name": "Standard",
              "columns": [
                "date",
                "subject",
                "category",
                "authors",
                "children"
              ]
            },
            {
              "name": "Mobil",
              "columns": [
                "date",
                "subject",
                "children"
              ]
            }
          ]
        },
        "filters": [
          {
            "id": "warningLevel"
          },
          {
            "id": "date",
            "default": 1,
            "options": [
              {
                "label": "Heute"
              },
              {
                "startOffsets": [
                  {
                    "amount": 0,
                    "unit": "weeks"
                  }
                ],
                "endOffsets": [
                  {
                    "amount": 0,
                    "unit": "weeks"
                  }
                ],
                "label": "Diese Woche"
              },
              {
                "startOffsets": [
                  {
                    "amount": -1,
                    "unit": "weeks"
                  }
                ],
                "label": "Seit letzter Woche"
              },
              {
                "startOffsets": [
                  {
                    "amount": 0,
                    "unit": "months"
                  }
                ],
                "endOffsets": [
                  {
                    "amount": 0,
                    "unit": "months"
                  }
                ],
                "label": "Diesen Monat"
              },
              {
                "startOffsets": [
                  {
                    "amount": -1,
                    "unit": "months"
                  }
                ],
                "endOffsets": [
                  {
                    "amount": -1,
                    "unit": "months"
                  }
                ],
                "label": "Letzten Monat"
              }
            ]
          },
          {
            "id": "category"
          },
          {
            "id": "authors"
          }
        ],
        "exportConfig": [
          {
            "label": "event_id",
            "query": "_id"
          },
          {
            "label": "date",
            "query": "date"
          },
          {
            "label": "event title",
            "query": "subject"
          },
          {
            "label": "event type",
            "query": "category"
          },
          {
            "label": "event description",
            "query": "text"
          },
          {
            "query": ":getAttendanceArray(true)",
            "subQueries": [
              {
                "query": ".participant:toEntities(Child)",
                "subQueries": [
                  {
                    "label": "participant_id",
                    "query": "_id"
                  },
                  {
                    "label": "participant",
                    "query": "name"
                  },
                  {
                    "label": "gender",
                    "query": "gender"
                  }
                ]
              },
              {
                "label": "status",
                "query": ".status._status.id"
              },
              {
                "query": ".school:toEntities(School)",
                "subQueries": [
                  {
                    "label": "school_name",
                    "query": "name"
                  },
                  {
                    "label": "school_id",
                    "query": "entityId"
                  }
                ]
              }
            ]
          }
        ]
      },
      "_id": "view:note"
    },
