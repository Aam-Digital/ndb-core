          "dataType": "long-text",
          "label": "Observations"
        },
        "authors": {
          "label": "Equipe impliquée",
          "dataType": "entity",
          "isArray": true,
          "additional": "User",
          "defaultValue": {
            "mode": "dynamic",
            "config": {
              "value": "$current_user"
            }
          },
          "anonymize": "retain"
        },
        "category": {
          "label": "Categorie",
          "dataType": "configurable-enum",
          "additional": "interaction-type",
