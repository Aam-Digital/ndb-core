          {
            "label": "event_id",
            "query": "_id"
          },
          {
            "label": "date",
            "query": "date"
          },
          {
            "label": "event title",
            "query": "subject"
          },
          {
            "label": "event type",
            "query": "category"
          },
          {
            "label": "event description",
            "query": "text"
          },
