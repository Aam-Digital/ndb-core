              {
                "startOffsets": [
                  {
                    "amount": 0,
                    "unit": "months"
                  }
                ],
                "endOffsets": [
                  {
                    "amount": 0,
                    "unit": "months"
                  }
                ],
                "label": "Ce mois-ci"
              },
              {
                "startOffsets": [
                  {
                    "amount": -1,
                    "unit": "months"
                  }
                ],
                "endOffsets": [
                  {
                    "amount": -1,
                    "unit": "months"
                  }
                ],
                "label": "Mois dernier"
              }
            ]
          },
          {
            "id": "category"
          },
          {
            "id": "authors"
          }
        ],
