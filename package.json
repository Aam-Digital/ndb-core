{
  "name": "ndb-core",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "build-localized": "ng build --configuration=production,localized",
    "test": "ng test",
    "test-ci": "ng test --code-coverage --karmaConfig=build/karma-ci.conf.js ",
    "lint": "ng lint ndb-core --type-check",
    "e2e-ci": "ng e2e --configuration=ci",
    "e2e": "ng e2e",
    "e2e-open": "ng run ndb-core-e2e:cypress-open",
    "compodoc": "npx compodoc -c doc/compodoc_sources/.compodocrc.json",
    "postinstall": "ngcc",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "extract-i18n": "ng extract-i18n --output-path ./src/locale/ && xliffmerge --profile xliffmerge.json",
    "cypress:open": "e2e open",
    "cypress:run": "e2e run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.3.11",
    "@angular/localize": "^13.3.11",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/router": "^13.3.11",
    "@angular/service-worker": "^13.3.11",
    "@casl/ability": "^5.4.3",
    "@casl/angular": "^7.0.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ngneat/until-destroy": "^8.1.4",
    "@sentry/browser": "^6.19.7",
    "angulartics2": "^10.1.0",
    "crypto-es": "^1.2.7",
    "deep-object-diff": "^1.1.7",
    "faker": "^5.5.3",
    "flag-icons": "^6.5.1",
    "hammerjs": "^2.0.8",
    "json-query": "^2.2.2",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "ngx-markdown": "^13.1.0",
    "ngx-papaparse": "^5.1.0",
    "pouchdb-browser": "^7.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular/cli": "^13.3.8",
    "@angular/compiler-cli": "^13.3.11",
    "@compodoc/compodoc": "1.1.11",
    "@cypress/schematic": "~1.5.1",
    "@schematics/angular": "^11.2.14",
    "@types/faker": "^5.5.7",
    "@types/file-saver": "^2.0.3",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "~3.8.2",
    "@types/json-query": "^2.2.3",
    "@types/lodash-es": "^4.17.6",
    "@types/md5": "^2.3.2",
    "@types/node": "^14.18.21",
    "@types/pouchdb": "^6.4.0",
    "@types/uuid": "^8.3.4",
    "codelyzer": "6.0.1",
    "cypress": "8.5.0",
    "husky": "^5.1.3",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.1",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^10.5.4",
    "ngx-i18nsupport": "^0.17.1",
    "prettier": "~2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.6.4"
  },
  "browser": {
    "crypto": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{ts,js,css}": [
      "tslint --fix"
    ]
  }
}
