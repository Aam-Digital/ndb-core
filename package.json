    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/angular": "^6.1.21",
    "@types/crypto-js": "^4.0.1",
    "@types/faker": "^5.5.0",
    "@types/file-saver": "^2.0.1",
