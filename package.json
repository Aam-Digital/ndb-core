  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "test": "ng test",
    "test-ci": "ng test --code-coverage --karma-config=build/karma-ci.conf.js ",
    "lint": "ng lint ndb-core",
    "e2e": "ng e2e",
