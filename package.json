    "typescript": "~4.6.4"
  },
  "browser": {
    "crypto": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{ts,js,css}": [
      "tslint --fix"
    ]
  }
