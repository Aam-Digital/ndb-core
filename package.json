    "faker": "^5.5.3",
    "flag-icon-css": "^3.5.0",
    "font-awesome": "^4.7.0",
    "idb": "^6.1.1",
    "json-query": "^2.2.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "ngx-filter-pipe": "^2.1.2",
    "ngx-markdown": "^11.1.3",
    "ngx-papaparse": "^5.0.0",
    "pouchdb-adapter-memory": "^7.2.2",
