    "ngx-i18nsupport": "^0.17.1",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.1.5",
    "webpack": "^4.6.0"
  },
  "browser": {
    "crypto": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
